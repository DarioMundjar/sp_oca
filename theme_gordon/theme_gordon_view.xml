<template id="theme_gordon_css" inherit_id="website.theme" customize_show="True" name="Snippet CSS"> 
    <xpath expr="//link[@href='/website/static/src/css/website.css']" position="after">

        <link rel="stylesheet" href="/theme_gordon/static/src/css/snippets.css" t-ignore="true"/>
        <link rel="stylesheet" href="/theme_gordon/static/src/css/custom.css" t-ignore="true"/>
        <link href='http://fonts.googleapis.com/css?family=Francois+One' rel='stylesheet' type='text/css' />
        <link href='http://fonts.googleapis.com/css?family=Cabin:400,400italic' rel='stylesheet' type='text/css' />
    </xpath>  
</template>


<template id="theme_gordon_header" inherit_id="website.layout" customize_show="True" name="Header"> 
    <xpath expr="//header" position="replace">
                <header class="main-header">
                    <div class="container full-wrapper">
                        <a href="/" class="navbar-brand" t-field="res_company.name" ></a>
                        <a href="javascript:void(0);" class="toggle-menu" data-toggle="collapse" data-target=".main-menu" >
                            <img src="/theme_gordon/static/src/img/hamburger.png" />
<!--
                            <span class="glyphicon glyphicon-menu-hamburger"></span>
-->
                        </a>
                        
                        <ul class="main-menu" role="navigation" id="top_menu">
                            <t t-foreach="website.menu_id.child_id" t-as="submenu">
                                <t t-call="website.submenu"/></t>
                            <li class="divider" t-ignore="true" t-if="website.user_id != user_id"/>
                            <li class="dropdown" t-ignore="true" t-if="website.user_id != user_id">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                    <b>
                                        <span t-esc="user_id.name"/>
                                        <span class="caret"/>
                                    </b>
                                </a>
                                <ul class="dropdown-menu js_usermenu" role="menu">
                                    <li>
                                        <a href="/web" role="menuitem">My Account</a>
                                    </li>
                                    <li class="divider"/>
                                    <li>
                                        <a t-attf-href="/web/session/logout?redirect=/" role="menuitem">Logout</a>
                                    </li>
                                </ul>
                            </li>
                        </ul>
                    </div>
              
                
                
                </header>
                
    </xpath>  
</template>



<template id="news_block"  name="News Block">         
                  <t t-set="news" t-value="request.env['blog.post'].search([('website_published','=',True)],order='write_date asc')"/>
                  
                  
                    <div class="grid-box-g" t-if="len(news)>0">
                      <t t-set="post" t-value="news[0]"/>
                      <img t-att-src="post.background_image" style=""/>
                      <div class="grid-box-overlay">
                        <div class="overlay-row">
                          <p class="overlay-title" ><t t-esc="post.name"/></p>
                          <span class="glyphicon glyphicon-chevron-down arrow-transform"/>
                        </div>
                        <div class="overlay-info">
                            
<!--
                         <h2><t t-esc="post.name"/></h2>
-->
                         <h2><t t-esc="post.subtitle"/></h2>
                         
                         <a class="btn btn-primary" t-attf-href="/blog/#{ slug(post.blog_id) }/post/#{ slug(post) }">Läs mer</a>
                         
                        </div>
                      </div>
                    
                </div>                
</template>

<template id="product_block"  name="Product Block">         
        
                  <t t-set="products" t-value="request.env['product.template'].search([('website_published','=',True)],order='website_sequence')"/>
                  
                    <div class="grid-box-g"  t-if="len(products)>0">
                      <t t-set="product" t-value="products[0]"/>
                      <img itemprop="image" class="img img-responsive" t-att-src="website.image_url(product, 'image', None if product_image_big else '300x300')"/>
                      <div class="grid-box-overlay">
                        <div class="overlay-row">
                          <p class="overlay-title" ><t t-esc="product.name"/></p>
                          <span class="glyphicon glyphicon-chevron-down arrow-transform"/>
                        </div>
                        <div class="overlay-info">
                            
                         <h2><t t-esc="product.description_sale"/></h2>
                         
                         <a class="btn btn-primary" t-attf-href="/shop/product/#{ product.id }">Läs mer</a>
                         
                        </div>
                      </div>
                    </div>
                
</template>

        
        

